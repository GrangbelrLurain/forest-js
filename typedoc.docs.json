{
  "out": "./docs",
  "entryPoints": ["packages/core/src/forest.ts", "packages/core/src/router/index.ts", "packages/core/src/store/index.ts", "packages/core/src/utilities/index.ts", "packages/core/src/types/index.ts"],
  "entryPointStrategy": "resolve",
  "excludePrivate": true,
  "excludeExternals": true,
  "includeVersion": true,
  "readme": "none",
  "name": "Forest.js Core Documentation",
  "categorizeByGroup": true,
  "sort": ["source-order"],
  "theme": "typedoc-github-theme",
  "plugin": ["typedoc-plugin-merge-modules", "typedoc-github-theme"],
  "mergeModulesMergeMode": "module",
  "categoryOrder": ["forest", "router", "store", "utilities", "types"],
  "validation": {
    "invalidLink": true,
    "notDocumented": true
  },
  "blockTags": ["@param", "@returns", "@example", "@template", "@property", "@type", "@default", "@description"]
}
